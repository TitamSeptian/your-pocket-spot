{
    "name": "Your pocket spot",
    "description": "using spot upi easier",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Your pocket spot",
        "default_icon": {
            "16": "src/icons/16.png",
            "32": "src/icons/32.png",
            "48": "src/icons/48.png",
            "128": "src/icons/128.png"
        }
    },
    "icons": {
        "16": "src/icons/16.png",
        "32": "src/icons/32.png",
        "48": "src/icons/48.png",
        "128": "src/icons/128.png"
    }
}
